
	<section class="container">
		<div id="new-link">
			<form action="/links" method="POST" id="new-link-form">
			<label for="title">Title</label> <input type="text" name="title">
			<label for="url">Url</label> <input type="text" name="url">
			<label for="tags">Tags</label> <input type="text" name="tags">
			<span><input type="submit" value="Add link" class="button"></span>
			</form>
		</div>
		<div class = "user-links">
		<ul>
		  <% @links.each do |link| %>
		    <dt><a href="<%= link.url %>"><%= link.title %></a></li>
		  <% end %>
		</ul>
		</div>
	</section>